{
  "extends": "@parcel/config-default",
  "transformers": {
    "*.hbs": ["parcel-transformer-handlebars-precompile"]
  },
  "validators": {
    "*.{ts,tsx}": [ "@parcel/validator-typescript", "@parcel/validator-eslint" ],
